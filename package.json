{
  "name": "agile-ui",
  "description": "A device discovery ui",
  "version": "0.0.1",
  "author": "Craig Mulligan <craig@resin.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/resin-io-playground/agile-ui.git"
  },
  "bugs": "https://github.com/resin-io-playground/issues",
  "scripts": {
    "clean:build": "node ./bin/clean.js createdir",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "node ./bin/clean.js",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "serve": "node app.js",
    "lint": "eslint config src/js/** --ext .js",
    "jscs": "jscs src/js/",
    "test": "mocha --compilers js:babel-core/register,css:./test/unit/cssNullCompiler.js --require ./test/unit/testHelper.js --recursive ./test/unit",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "babel-core": "6.7.7",
    "babel-eslint": "6.0.3",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "chai": "3.5.0",
    "chai-jquery": "2.0.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "2.1.3",
    "css-loader": "0.23.1",
    "eslint": "2.8.0",
    "eslint-plugin-react": "5.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jquery": "2.2.2",
    "jscs": "2.10.1",
    "jsdom": "8.2.0",
    "mocha": "2.4.5",
    "node-sass": "3.7.0",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "15.0.1",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-slider-monitor": "1.0.5",
    "rimraf": "2.5.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "axios": "0.14.0",
    "babel-preset-react": "^6.5.0",
    "compression": "^1.6.2",
    "express": "4.14.0",
    "history": "2.1.0",
    "http-proxy": "1.14.0",
    "humps": "1.1.0",
    "lodash": "4.11.1",
    "material-ui": "0.15.4",
    "normalizr": "2.2.1",
    "random-material-color": "1.0.2",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-saga": "0.11.0",
    "serve-static": "^1.11.1"
  }
}
