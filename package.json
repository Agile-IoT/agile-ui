    "compression": "1.7.1",
    "express": "4.16.2",
    "express-bearer-token": "2.1.0",
    "http-proxy-middleware": "0.17.4",
    "lodash": "4.17.4",
    "material-ui": "0.17.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-edit-inline": "^1.0.8",
    "react-jsonschema-form": "^0.51.0",
