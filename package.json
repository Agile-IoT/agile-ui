    "react-router": "3.0.2",
    "react-tap-event-plugin": "2.0.1",
    "react-tooltip": "^3.4.0",
    "recursive-keys": "^0.9.0",
    "redux": "3.6.0",
    "redux-localstorage": "0.4.1",
    "redux-thunk": "2.2.0",
    "underscore": "^1.8.3"
